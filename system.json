{
  "id": "aafo",
  "title": "AAFO",
  "description": "Game system for the Arcane Arcade fallout system",
  "version": "0.0.1",
  "compatibility": {
    "minimum": "11.315",
    "verified": "12"
  },
  "url": "https://github.com/foundryvtt/aafo/",
  "manifest": "https://raw.githubusercontent.com/foundryvtt/aafo/master/system.json",
  "download": "https://github.com/foundryvtt/aafo/releases/download/release-3.2.0/aafo-release-3.2.0.zip",
  "authors": [
    {
      "name": "Atropos",
      "url": "https://foundryvtt.com",
      "flags": {}
    }
  ],
  "esmodules": [
    "aafo.mjs"
  ],
  "styles": [
    "aafo.css"
  ],
  "packs": [
    {
      "name": "heroes",
      "label": "Starter Heroes",
      "system": "aafo",
      "path": "packs/heroes",
      "type": "Actor",
      "private": false,
      "flags": {}
    },
    {
      "name": "monsters",
      "label": "Monsters (SRD)",
      "system": "aafo",
      "path": "packs/monsters",
      "type": "Actor",
      "private": false,
      "flags": {}
    },
    {
      "name": "items",
      "label": "Items (SRD)",
      "system": "aafo",
      "path": "packs/items",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "tradegoods",
      "label": "Trade Goods (SRD)",
      "system": "aafo",
      "path": "packs/tradegoods",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "spells",
      "label": "Spells (SRD)",
      "system": "aafo",
      "path": "packs/spells",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "backgrounds",
      "label": "Backgrounds (SRD)",
      "system": "aafo",
      "path": "packs/backgrounds",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "classes",
      "label": "Classes (SRD)",
      "system": "aafo",
      "path": "packs/classes",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "subclasses",
      "label": "Subclasses (SRD)",
      "system": "aafo",
      "path": "packs/subclasses",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "classfeatures",
      "label": "Class & Subclass Features (SRD)",
      "system": "aafo",
      "path": "packs/classfeatures",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "perks",
      "label": "Perks",
      "system": "aafo",
      "path": "packs/perks",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "races",
      "label": "Races (SRD)",
      "system": "aafo",
      "path": "packs/races",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "monsterfeatures",
      "label": "Monster Features (SRD)",
      "system": "aafo",
      "path": "packs/monsterfeatures",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "rules",
      "label": "Rules (SRD)",
      "system": "aafo",
      "path": "packs/rules",
      "type": "JournalEntry",
      "private": false,
      "flags": {
        "display": "table-of-contents"
      }
    },
    {
      "name": "rulesaafo",
      "label": "AAFO Rules",
      "system": "aafo",
      "path": "packs/rulesaafo",
      "type": "JournalEntry",
      "private": false,
      "flags": {
        "display": "table-of-contents"
      }
    },
    {
      "name": "tables",
      "label": "Tables (SRD)",
      "system": "aafo",
      "path": "packs/tables",
      "type": "RollTable",
      "private": false,
      "flags": {}
    }
  ],
  "packFolders": [
    {
      "name": "D&D SRD Content",
      "sorting": "m",
      "color": "#cd2c1e",
      "packs": [
        "rules",
        "tables",
        "heroes"
      ],
      "folders": [
        {
          "name": "Character Features",
          "sorting": "m",
          "color": "#b72c1f",
          "packs": [
            "classes",
            "subclasses",
            "classfeatures",
            "backgrounds",
            "races"
          ]
        },
        {
          "name": "Items & Spells",
          "sorting": "m",
          "color": "#941919 ",
          "packs": [
            "items",
            "spells",
            "tradegoods"
          ]
        },
        {
          "name": "Monsters",
          "sorting": "m",
          "color": "#7d160c",
          "packs": [
            "monsters",
            "monsterfeatures"
          ]
        }
      ]
    },
    {
      "name": "AAFO SRD",
      "sorting": "m",
      "color": "#cd2c1e",
      "packs": [
        "rulesaafo"
      ],
      "folders": [
        {
          "name": "Character Features",
          "sorting": "m",
          "color": "#b72c1f",
          "packs": [
            "perks"
          ]
        }
      ]
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    }
  ],
  "media": [
    {
      "type": "cover",
      "loop": false,
      "flags": {}
    },
    {
      "type": "setup",
      "url": "systems/aafo/ui/official/aafo-repo.jpg",
      "thumbnail": "systems/aafo/ui/official/aafo-thumbnail.jpg"
    }
  ],
  "socket": true,
  "gridDistance": 5,
  "gridUnits": "ft",
  "grid": {
    "distance": 5,
    "units": "ft"
  },
  "primaryTokenAttribute": "attributes.hp",
  "background": "systems/aafo/ui/official/aafo-background.webp",
  "flags": {
    "needsMigrationVersion": "3.1.0",
    "compatibleMigrationVersion": "0.8",
    "hotReload": {
      "extensions": ["css", "hbs", "json"],
      "paths": ["aafo.css", "templates", "lang"]
    }
  }
}
